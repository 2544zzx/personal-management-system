{% set classes = ( classes is not defined ? "" : classes ) %}
{% set checked = ( checked is not defined ? "" : checked ) %}
{% set label   = ( label is not defined   ? "" : label ) %}

<div class="checkbox checkbox-circle checkbox-info peers ai-c">
    <input type="checkbox" name="inputCheckboxesCall" class="peer {{ classes }}" {{ checked }}>
    <label class=" peers peer-greed js-sb ai-c">

            <span class="peer peer-greed">
                {{ label }}
            </span>

    </label>
</div>
