<table id="MyCarSchedulesTypes" class="table table-striped table-bordered" cellspacing="0" data-table="true" width="100%">
    <thead>
    <tr>
        <td class="d-none">Id</td>
        <td>Name</td>
        <td>Actions</td>
    </tr>
    </thead>
    <tbody>
    {% for type in types %}
        <tr class="trash-parent car-single-data editable-parent save-parent" data-type="MyCarSchedulesTypes">
            <td class="d-none id">{{ type.id }}</td>
            <td class="name"> {{ type.name }}</td>

            <td contenteditable="false">
                <i class="fa fa-trash" aria-hidden="true"></i>
                <i class="far fa-edit"></i>
                <i class="far fa-save d-none"></i>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
