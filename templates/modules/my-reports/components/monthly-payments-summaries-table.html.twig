<div>

    <table id="MyPaymentsReportsMonthlyPaymentsSummaries" class="table table-hover" cellspacing="0" width="100%" data-table="true">
        <thead>
        <tr>
            <td>{{ translate('reports.monthlyPaymentsSummaries.table.monthAndYear') }}</td>
            <td>{{ translate('reports.monthlyPaymentsSummaries.table.money') }}</td>
        </tr>
        </thead>
        <tbody>
        {% for row in data %}
            <tr data-type="MyPaymentsReportsMonthlyPaymentsSummaries">
                <td> {{ row['yearAndMonth'] }} </td>
                <td> {{ row['money'] }} </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

</div>