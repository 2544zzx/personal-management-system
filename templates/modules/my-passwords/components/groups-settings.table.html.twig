<table id="MyPasswordsGroups" class="table table-striped table-bordered" cellspacing="0" data-table="true" width="100%">
    <thead>
    <tr>
        <td class="d-none">{{ translate('datatables.table.id') }}</td>
        <td>{{ translate('passwords.settings.table.name') }}</td>
        <td>{{ translate('datatables.table.actions') }}</td>
    </tr>
    </thead>
    <tbody>
    {% for group in groups %}
        <tr class="trash-parent car-single-data editable-parent save-parent" data-type="MyPasswordsGroups">
            <td class="d-none id">{{ group.id }}</td>
            <td class="name"> {{ group.name }}</td>

            <td contenteditable="false">
                <i class="fa fa-trash" aria-hidden="true"></i>
                <i class="far fa-edit"></i>
                <i class="far fa-save d-none"></i>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
