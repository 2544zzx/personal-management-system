<div>
    <table id="MyShoppingPlans" class="table table-striped table-bordered" cellspacing="0"
           data-table="true" width="100%">
        <thead>
        <tr>
            {% for column in columns_names %}
                <td class="{% if column=='money' %}text-success{% endif %} {{ hideIdColumn(column) }}">
                    {{ column|capitalize }}
                </td>
            {% endfor %}
            <td>{{ translate('datatables.table.actions') }}</td>
        </tr>
        </thead>
        <tbody>
        {% for plan in all_plans %}
            <tr class="trash-parent monthly-single-data editable-parent save-parent" data-type="MyShoppingPlans">
                {% for column in columns_names %}
                    <td class="{{ column }} {% if column=='money' %}text-success{% endif %} {{ hideIdColumn(column) }}">
                        {{ attribute(plan,column) }}
                    </td>
                {% endfor %}
                <td contenteditable="false">
                    <i class="fa fa-trash" aria-hidden="true"></i>
                    <i class="far fa-edit"></i>
                    <i class="far fa-save d-none"></i>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>